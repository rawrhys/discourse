{
  "name": "discourse",
  "version": "1.0.0",
  "type": "module",
  "description": "Discourse Learning Management System",
  "main": "server.js",
  "scripts": {
    "start": "pm2 start ecosystem.config.js",
    "stop": "pm2 stop ecosystem.config.js",
    "restart": "pm2 restart ecosystem.config.js",
    "reload": "pm2 reload ecosystem.config.js",
    "delete": "pm2 delete ecosystem.config.js",
    "logs": "pm2 logs discourse-app",
    "status": "pm2 status",
    "monit": "pm2 monit",
    "dev": "vite",
    "build": "node --max-old-space-size=4096 node_modules/vite/dist/node/cli.js build && node compress.js",
    "build:clean": "rm -rf dist node_modules/.vite && npm run build",
    "build:windows": "node --max-old-space-size=4096 node_modules/vite/dist/node/cli.js build && node compress.js",
    "preview": "vite preview",
    "start:dev": "cross-env NODE_ENV=development pm2 start ecosystem.config.js --env development",
    "test": "node server.test.js"
  },
  "dependencies": {
    "@hcaptcha/react-hcaptcha": "^1.12.1",
    "@stripe/stripe-js": "^7.9.0",
    "@supabase/supabase-js": "^2.45.0",
    "async": "^3.2.6",
    "axios": "^1.7.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-fileupload": "^1.5.2",
    "express-rate-limit": "^7.1.5",
    "express-slow-down": "^2.0.1",
    "framer-motion": "^12.23.12",
    "glob": "^11.0.3",
    "helmet": "^7.1.0",
    "lowdb": "^7.0.1",
    "markdown-it": "^14.1.0",
    "marked": "^16.1.2",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.5",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.2.1",
    "react-router-dom": "^6.25.1",
    "react-window": "^1.8.11",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "selfsigned": "^2.4.1",
    "sharp": "^0.34.3",
    "speak-tts": "^2.0.8",
    "stripe": "^17.7.0",
    "user-agents": "^1.0.35",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.40",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^5.8.0",
    "supertest": "^7.0.0",
    "tailwindcss": "^3.4.7",
    "tape": "^5.8.1",
    "terser": "^5.43.1",
    "vite": "^6.3.5",
    "vite-plugin-compression": "^0.5.1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
